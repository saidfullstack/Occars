{{ form_start(form) }}
<div class="row">
    <div class="col-md-5">{{ form_row(form.piece) }}</div>
    <div class="col-md-5">{{ form_row(form.marque) }}</div>
    <div class="col-md-5">{{ form_row(form.modele) }}</div>
    <div class="col-md-5">{{ form_row(form.energie) }}</div>
    <div class="col-md-4">{{ form_row(form.image) }}</div>
    <div class="col-md-3">{{ form_row(form.disponible) }}</div>
    <div class="col-md-3">{{ form_row(form.prix) }}</div>
</div>
<div class="row">
    <div class="col-md-10">{{ form_row(form.description) }}</div>
</div>
{{ form_rest(form) }}
<button class="btn btn-primary">{{ button|default('Enregistrer') }}</button>
{{ form_end(form) }}
